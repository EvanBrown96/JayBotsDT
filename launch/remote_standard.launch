<launch>

	<arg name="machine_name" />
	<arg name="ip_addr" />

	<machine name="$(arg machine_name)" address="$(arg ip_addr)" user="pmr" env-loader="~/JayBotsDT/jaybot_ws/devel/env.sh" />

	<node machine="$(arg machine_name)" name="driver" pkg="jaybot" type="driver_node.py" />
	<node machine="$(arg machine_name)" name="movement_logic" pkg="jaybot" type="movement_logic.py" />
	<node machine="$(arg machine_name)" name="sonar_array" pkg="jaybot" type="avoidance.py" />

</launch>
